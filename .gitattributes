# Set the default behavior, in case people don't have core.autocrlf set.
* text=auto


*.bat      text eol=crlf
*.c        text eol=crlf
*.cpp      text eol=crlf
*.csv      text eol=crlf
*.css      text eol=lf
*.ebp      text eol=lf
*.doxyfile text eol=crlf
*.h        text eol=crlf
*.html     text eol=crlf
*.js       text eol=lf
*.ld       text eol=crlf
*.ini      text eol=crlf
*.php      text eol=crlf
*.md       text eol=crlf
*.txt      text eol=crlf
*.rc       text eol=crlf
*.s        text eol=crlf
*.S        text eol=crlf
*.svg      text eol=lf
*.u*       text eol=lf
*.xml      text eol=crlf
*.wxs      text eol=crlf

# Denote all files that are truly binary and should not be modified.
*.exe binary
*.a   binary
*.ico binary
